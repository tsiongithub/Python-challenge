num = int(input("Numbers you want? "))

num1 = 0
num2 = 1
num3 = int()
counter = 0

if num <= 0:
    print("enter any positive number")

else:
    print("Fibonacci sequence:")

    while counter < num:
        print(num1)
        num3 = num1 + num2

        num1 = num2
        num2 = num3
        counter +=1
